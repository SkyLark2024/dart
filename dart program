void main() {
  // Double data type
  double pi = 3.14159;
  print('Value of Pi: $pi');

  // String data type
  String greeting = 'Hello, Dart!';
  print('Greeting: $greeting');

  // List data type
  List<int> numbers = [1, 2, 3, 4, 5];
  print('Numbers List: $numbers');

  // Map data type
  Map<String, dynamic> person = {
    'name': 'Alice',
    'age': 30,
    'isStudent': false,
  };
  print('Person Details: $person');
}
